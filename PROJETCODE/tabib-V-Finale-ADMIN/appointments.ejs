						<div class="table-responsive">
							<table class="table table-border table-striped custom-table datatable mb-0">
								<thead>
									<tr>
										<th>Appointment ID</th>
										<th>Patient Name</th>
										<th>Age</th>
										<th>Doctor Name</th>
                                        <th>Specialite</th>
                                        <th>Date</th>
										<th class="text-right">Action</th>
									</tr>
								</thead>
								<tbody>
                                        <% infos.forEach(info=>{
                                            info.forEach(info=>{ %>
    
                                            
                                        <tr>
                                            <td><%=info.id%></td>
                                            <td><img width="28" height="28" src="img/<%=info.phprofilepatient%>" class="rounded-circle m-r-5" alt=""><%=info.nompatient%> <%=info.prenompatient%></td>
                                            <td><%=info.agepatient%></td>
                                            <td><%=info.nomdoctor%> <%=info.prenomdoctor%></td>
                                            <td><%=info.specialitedoctor%></td>
                                            <td><%=info.jour%></td>
                                            
                                            
                                            <td class="text-right">
                                                <div class="dropdown dropdown-action">
                                                    <a href="appointments.html#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                                    <div class="dropdown-menu dropdown-menu-right">
                                                        <a class="dropdown-item" href="edit-appointment.html"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                        <a class="dropdown-item" href="appointments.html#" data-toggle="modal" data-target="#delete_appointment"><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                        <%})});%>
									
								</tbody>
							</table>
